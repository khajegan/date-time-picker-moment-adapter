{
    "name": "@khajegan/angular-datetime-picker-moment-adapter",
    "version": "17.0.0",
    "description": "Angular Date Time Picker (MomentJs Adapter)",
    "keywords": [
        "Angular",
        "datepicker",
        "date picker",
        "timepicker",
        "time picker",
        "datetime picker",
        "date time picker",
        "material",
        "ngx",
        "moment",
        "momentjs"
    ],
    "author": "Maintained and updated by Daniel Moncada, original implementatiom by Daniel Pan",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build picker",
        "test": "ng test picker",
        "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build_lib": "ng build picker --configuration production && cp README.md dist/picker",
        "npm_pack": "cd dist/picker && npm pack",
        "package_windows": "npm run build_lib && npm run npm_pack",
        "package_linux": "npm run build_lib && npm run npm_pack",
        "myScope": "cd projects/picker && sudo npm init --scope=@khajegan",
        "myBuild": "sudo ng b picker --configuration production && cp README.md dist/picker",
        "myPublish": "cd dist/picker && npm publish --access public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/khajegan/date-time-picker-moment-adapter.git"
    },
    "dependencies": {
        "@angular/animations": "^14.0.3",
        "@angular/common": "^14.0.3",
        "@angular/compiler": "^14.0.3",
        "@angular/core": "^14.0.3",
        "@angular/forms": "^14.0.3",
        "@angular/platform-browser": "^14.0.3",
        "@angular/platform-browser-dynamic": "^14.0.3",
        "@angular/router": "^14.0.3",
        "moment": "^2.29.3",
        "moment-timezone": "^0.5.34",
        "np": "^6.2.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.3",
        "@angular/cdk": "^14.0.3",
        "@angular/cli": "^14.0.3",
        "@angular/compiler-cli": "^14.0.3",
        "@angular/language-service": "^14.0.3",
        "@danielmoncada/angular-datetime-picker": "^14.1.0",
        "@types/jasmine": "^3.10.0",
        "@types/jasminewd2": "^2.0.10",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^12.12.29",
        "codecov": "^3.6.5",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~4.0.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "protractor": "~7.0.0",
        "ts-node": "^8.6.2",
        "tslint": "~6.1.0",
        "typescript": "^4.7.4"
    },
    "peerDependencies": {
        "@angular/cdk": "^13.0.3 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0",
        "@angular/common": "^13.0.3 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0",
        "@angular/core": "^13.0.3 || ^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"
    }
}
